<Window x:Class="KoleServis.MVVM.View.CustomMessageBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KoleServis.MVVM.View"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Info" Height="200" Width="300">
    <Grid Background="{DynamicResource OverallBackground}">
        <TextBlock Text="{DynamicResource NotFound}" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="16"/>
        <Button  HorizontalAlignment="Center" VerticalAlignment="Bottom" Width="75" Height="30" Click="OnOkClicked">
            <TextBlock Text="OK" Style="{StaticResource CommandText}"/>
        </Button>
    </Grid>

</Window>
